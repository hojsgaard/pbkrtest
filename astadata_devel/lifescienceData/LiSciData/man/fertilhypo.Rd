data(fertilhypo)
#data generated by
set.seed(8)
treat  <-  rep(c(1,  2,  3),  each  = 10)
##treat  <-  rep(c(1,  2,  3),  each  =  c(10,  11,  12))

x  <-  1:length(treat)
treatf  <-  factor(treat)
yA  <-  5  +  rnorm(x,  sd  =  2.3)
yA  <-  ifelse(yA  <  0,  0.55,  yA)
yB  <-  5  +  rnorm(x,  sd  =  0.6)
yC  <-  c(2,  5,  9)[treatf]  +  rnorm(x,  sd  =  0.6)
}
